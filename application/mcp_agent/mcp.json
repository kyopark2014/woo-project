{
    "mcpServers": {
        "knowledge_base": {
            "command": "python",
            "args": ["application/mcp_agent/mcp_server_retrieve.py"]
        },
        "repl_coder": {
            "command": "python",
            "args": ["application/mcp_agent/mcp_server_repl_coder.py"]
        },
        "awslabs.aws-documentation-mcp-server": {
            "command": "uvx",
            "args": ["awslabs.aws-documentation-mcp-server@latest"],
            "env": {
                "FASTMCP_LOG_LEVEL": "ERROR"
            }
        },
        "qa_agent": {
            "command": "python",
            "args": ["application/mcp_server_qa_agent.py"]
        },
        "google_workspace": {
            "command": "uvx",
            "args": ["workspace-mcp"],
            "env": {
                "GOOGLE_OAUTH_CLIENT_ID": "your-client-id.apps.googleusercontent.com",
                "GOOGLE_OAUTH_CLIENT_SECRET": "your-client-secret",
                "OAUTHLIB_INSECURE_TRANSPORT": "1"
            }
        },
        "filesystem": {
            "command": "npx",
            "args": [
                "@modelcontextprotocol/server-filesystem",
                "/Users/ksdyb/Downloads/woo/rag"
            ]
        }
    }
}